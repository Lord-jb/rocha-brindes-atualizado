{
  "name": "rocha-brindes-astro-solid",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "pages:dev": "wrangler pages dev dist",
    "pages:deploy": "npm run build && wrangler pages deploy dist",
    "db:init": "wrangler d1 execute rocha-brindes-db --file=./schema.sql",
    "db:migrate": "wrangler d1 execute rocha-brindes-db --file=./schema.sql"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.2",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/solid-js": "^4.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@solidjs/router": "^0.14.0",
    "@tanstack/solid-query": "^5.56.0",
    "jszip": "^3.10.1",
    "lucide-solid": "^0.456.0",
    "solid-js": "^1.9.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "astro": "^5.16.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.87.0"
  }
}
